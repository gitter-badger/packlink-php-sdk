{
	"license" : "AGPL-3.0+",
	"homepage" : "https://angelguzmanmaeso.com",
	"type" : "library",
	"description" : "A PHP library for implement the Packlink API REST service from Packlink Shipping, S.L.",
	"name" : "shakaran/packlink-php-sdk",
	"minimum-stability": "dev",
	"authors" : [{
			"name" : "Ángel Guzmán Maeso",
			"email" : "angel@guzmanmaeso.com",
			"homepage" : "https://angelguzmanmaeso.com",
			"role" : "Developer"
		}
	],
	"support" : {
		"email" : "angel@guzmanmaeso.com"
	},
	"require" : {
		"php": "^7.1",
		"guzzlehttp/guzzle": "^6.3",
		"guzzlehttp/psr7": "^1.5",
		"kriswallsmith/buzz": "^1.0",
		"nyholm/psr7": "^1.1",
		"php-http/client-common": "^1.9 || ^2.0",
		"php-http/client-implementation": "1.0",
		"php-http/curl-client": "dev-master",
		"php-http/discovery": "^1.6",
		"php-http/httplug": "dev-master",
		"php-http/message": "^1.7",
		"php-http/multipart-stream-builder": "^1.0",
		"psr/http-client": "^1.0"
	},
	"require-dev" : {
		"phpunit/phpunit": "~6.0",
		"puli/composer-plugin": "^1.0@beta"
	},
	"keywords" : [
		"delevery",
		"shipment",
		"export",
		"import",
		"Packlink"
	],
	"config" : {
		"sort-packages" : true
	},
	"autoload" : {
		"psr-0" : {
			"PackLink\\" : "src/"
		},
		"psr-4" : {
			"PackLink\\" : "src/"
		}
	},
	"autoload-dev" : {
		"psr-0" : {
			"PackLinkTest\\" : "test/"
		}
	}
}
